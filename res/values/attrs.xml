<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable
		name="ArtGallery">
		<attr
			name="android:galleryItemBackground" />
	</declare-styleable>

	<declare-styleable
		name="ArtField">
		<attr
			name="labelLeft"
			format="boolean" />
		<attr
			name="multiLine"
			format="boolean" />
		<attr
			name="minLines"
			format="integer" />
		<attr
			name="colorBgNormal"
			format="reference|color" />
		<attr
			name="colorBgEditing"
			format="reference|color" />
		<attr
			name="colorTxtNormal"
			format="reference|color" />
		<attr
			name="colorTxtEditing"
			format="reference|color" />
		<attr
			name="inputType"
			format="enum">
			<enum
				name="text" value="1" />
			<enum
				name="number" value="2" />
		</attr>
	</declare-styleable>
</resources>